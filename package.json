{
    "name": "upper_winds_open_meteo",
    "version": "2.0.0",
    "description": "DZMAster skydiving tool",
    "type": "module",
    "scripts": {
        "start": "vite",
        "dev:mobile": "vite --config vite.config.mobile.js",
        "build:web": "vite build",
        "build:mobile": "vite build --config vite.config.mobile.js",
        "build": "npm run build:web && npm run build:mobile",
        "predeploy": "npm run build:web",
        "deploy": "gh-pages -d dist/web",
        "deploy-cloud" : "vite build --base=/apps/dzmaster/  && (cd dist/web;     rsync -avz --delete . heidi@cloud:/var/www/heidi.mah.priv.at/apps/dzmaster/ )",
        "test": "vitest"
    },
    "devDependencies": {
        "@capacitor/assets": "^3.0.5",
        "@playwright/test": "^1.55.0",
        "@rollup/plugin-commonjs": "^28.0.5",
        "gh-pages": "^6.3.0",
        "http-server": "^14.1.1",
        "vite": "^6.3.5",
        "vitest": "^3.2.4"
    },
    "dependencies": {
        "@capacitor-community/background-geolocation": "^1.2.25",
        "@capacitor/android": "^7.4.0",
        "@capacitor/cli": "^7.3.0",
        "@capacitor/core": "^7.4.2",
        "@capacitor/filesystem": "^7.1.2",
        "@capacitor/geolocation": "^7.1.4",
        "@capacitor/ios": "^7.4.2",
        "@capacitor/splash-screen": "^7.0.2",
        "@capawesome/capacitor-android-edge-to-edge-support": "^7.2.3",
        "@capawesome/capacitor-file-picker": "^7.2.0",
        "leaflet": "^1.9.4",
        "leaflet-gpx": "^2.2.0",
        "leaflet-rotatedmarker": "^0.2.0",
        "leaflet.heat": "^0.2.0",
        "luxon": "^3.6.1",
        "mgrs": "^2.1.0",
        "papaparse": "^5.5.3"
    },
    "main": "app.js",
    "keywords": [],
